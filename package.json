{
  "name": "gently-copy",
  "version": "3.2.0",
  "description": "Copy files without overwriting",
  "license": "MIT",
  "repository": "dmitriz/gently-copy",
  "author": {
    "name": "Dmitri Zaitsev",
    "email": "dmitri14@gmail.com",
    "url": "github.com/dmitriz"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "start": "cd demo/ && node demo.js && cd ../",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git push origin --all; git push origin --tags",
    "test": "nyc ava",
    "coverage": "node_modules/.bin/nyc --reporter=html --report-dir coverage ava",
    "pub": "npm version patch && npm publish && git push",
    "sync": "git ac && git pull --rebase && git push"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "copy",
    "file",
    "file-system",
    "overwrite",
    "directory",
    "safe",
    "graceful",
    "gentle",
    "inform"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "codecov": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "nyc": "^13.1.0",
    "tap": "^12.1.0"
  },
  "bugs": {
    "url": "https://github.com/dmitriz/gently-copy/issues"
  }
}
